    "license": "MIT",
    "authors": [
        {
            "name": "Michael Ishola",
            "email": "myckhel1@hotmail.com",
            "homepage": "https://myckhel.adbin.com.ng"
        }
    ],
    "homepage": "https://github.com/myckhel/laravel-vtpass",
    "keywords": ["Laravel", "Vtpass"],
    "require": {
        "illuminate/support": "~5|~6|~7"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Myckhel\\Vtpass\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Myckhel\\Vtpass\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Myckhel\\Vtpass\\VtpassServiceProvider"
            ],
            "aliases": {
                "Vtpass": "Myckhel\\Vtpass\\Facades\\Vtpass"
            }
        }
    }
}
